{
  "cli": {
    "version": ">= 13.0.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "android": { "buildType": "apk" },
      "env": { "EXPO_NO_TELEMETRY": "1" }
    },
    "preview": {
      "distribution": "internal",
      "channel": "preview",
      "env": { "EXPO_NO_TELEMETRY": "1" }
    },
    "production": {
      "channel": "production",
      "android": { "buildType": "app-bundle" },
      "env": { "EXPO_NO_TELEMETRY": "1" }
    }
  },
  "submit": {
    "production": {
      "android": {
        "serviceAccountKeyPath": "./google-service-account.json",
        "track": "internal"
      },
      "ios": {}
    }
  }
}